<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>be.roget.brewUpdate</string>
		<key>ProgramArguments</key>
		<array>
			<string>/bin/bash</string>
			<string>/usr/local/var/homebrew-auto-update/brew-update-notifier.sh</string>
		</array>
		<key>StandardOutPath</key>
		<string>/usr/local/var/homebrew-auto-update/brew-update-notifier.log</string>
		<key>StandardErrorPath</key>
		<string>/usr/local/var/homebrew-auto-update/brew-update-notifier.error.log</string>
		<key>RunAtLoad</key>
		<true/>
		<key>StartCalendarInterval</key>
		<dict>
			<key>Hour</key>
			<integer>12</integer>
			<key>Minute</key>
			<integer>00</integer>
		</dict>
	</dict>
</plist>
