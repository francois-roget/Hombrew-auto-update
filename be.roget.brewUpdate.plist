<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>be.roget.brewUpdate</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>/Users/fro/scripts/bin/brew-update-notifier.sh</string>
	</array>
	<key>StandardOutPath</key>
	<string>/Users/fro/scripts/logs/brew-update-notifier.log</string>
